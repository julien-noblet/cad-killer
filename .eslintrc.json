{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": ["plugin:flowtype/recommended"],
  "globals": {},
  "plugins": ["flowtype"],
  "rules": {
    "no-console": ["warn"],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true,
        "allow": ["_id", "_latlng"]
      }
    ]

  }
}