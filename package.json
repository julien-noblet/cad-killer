{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "watch": "webpack --watch",
    "start": "webpack serve --open",
    "build": "npx webpack --mode production --config webpack.config.js --progress --color",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:scss:fix": "stylelint '**/*.scss' --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "update-screenshots": "node --experimental-vm-modules node_modules/jest/bin/jest.js -u"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --insert-pragma",
      "git add"
    ],
    "*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "src/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": [
    "osm"
  ],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@babel/plugin-transform-runtime": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-stage-0": "7.8.3",
    "@babel/runtime": "7.28.4",
    "@babel/runtime-corejs2": "7.28.4",
    "@eslint/js": "^9.39.1",
    "babel-7-jest": "21.3.3",
    "babel-loader": "10.0.0",
    "commitizen": "4.3.1",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.4",
    "cssnano": "7.1.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "file-loader": "6.2.0",
    "globals": "^17.0.0",
    "handlebars": "4.7.8",
    "html-webpack-plugin": "5.6.5",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-image-snapshot": "6.5.1",
    "jest-puppeteer": "11.0.0",
    "lint-staged": "16.2.7",
    "mini-css-extract-plugin": "2.9.4",
    "postcss": "8.5.6",
    "postcss-import": "16.1.1",
    "postcss-loader": "8.2.0",
    "postcss-preset-env": "10.6.1",
    "prettier": "3.7.4",
    "prettier-eslint": "16.4.2",
    "puppeteer": "24.34.0",
    "regenerator-runtime": "0.14.1",
    "sass": "^1.71.1",
    "sass-loader": "16.0.6",
    "style-loader": "4.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "tmp": "^0.2.5",
    "webpack-dev-server": "5.2.2"
  },
  "dependencies": {
    "@fontsource-variable/open-sans": "^5.2.7",
    "es6-promise": "4.2.8",
    "leaflet": "1.9.4",
    "leaflet-ajax": "2.1.0",
    "leaflet-dialog": "1.0.5",
    "leaflet-draw": "1.0.4",
    "leaflet-hash": "0.2.1",
    "leaflet-modal": "0.2.0",
    "leaflet-plugins": "3.4.0",
    "leaflet.browser.print": "2.0.2",
    "leaflet.photon": "0.9.2",
    "material-design-iconic-font": "2.2.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^6.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "sandbox",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "preset": "jest-puppeteer",
    "globalSetup": "./setup.js",
    "globalTeardown": "./teardown.js",
    "testEnvironment": "./puppeteer_environment.js",
    "globals": {
      "window": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}