{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "start": "webpack serve --https --mode development",
    "build": "npx webpack --mode production --config webpack.config.js --progress --color",
    "test": "flow && jest --runInBand"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --insert-pragma",
      "git add"
    ],
    "*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "src/**/*.flow": [
      "prettier --parser flow --write",
      "git add"
    ],
    "src/*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "src/scss/*.scss": [
      "prettier --parser flow --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": [
    "osm"
  ],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-flow-strip-types": "7.16.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-flow": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-stage-0": "7.8.3",
    "@babel/runtime": "7.17.0",
    "@babel/runtime-corejs2": "7.17.0",
    "babel-7-jest": "21.3.3",
    "babel-loader": "8.2.3",
    "commitizen": "4.2.4",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "css-module-flow": "1.0.0",
    "cssnano": "5.0.17",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "file-loader": "6.2.0",
    "flow-bin": "0.171.0",
    "flow-typed": "3.6.1",
    "handlebars": "4.7.7",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "imagemin-webpack-plugin": "2.4.2",
    "jest": "27.5.0",
    "jest-image-snapshot": "4.5.1",
    "jest-puppeteer": "6.1.0",
    "lint-staged": "12.3.3",
    "mini-css-extract-plugin": "2.5.3",
    "node-sass": "7.0.1",
    "postcss": "8.4.6",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.3.1",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "puppeteer": "13.2.0",
    "regenerator-runtime": "0.13.9",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1"
  },
  "dependencies": {
    "es6-promise": "4.2.8",
    "jquery": "3.6.0",
    "leaflet": "1.7.1",
    "leaflet-ajax": "2.1.0",
    "leaflet-dialog": "1.0.5",
    "leaflet-draw": "1.0.4",
    "leaflet-hash": "0.2.1",
    "leaflet-modal": "0.2.0",
    "leaflet-plugins": "3.4.0",
    "leaflet.browser.print": "2.0.0",
    "leaflet.photon": "0.8.0",
    "material-design-iconic-font": "2.2.0",
    "opensans-npm-webfont": "1.0.0",
    "pouchdb": "7.2.2",
    "ua-parser-js": "1.0.2",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "sandbox",
      "node_modules"
    ],
    "transform": {
      "^.+\\.js?$": "babel-7-jest"
    },
    "preset": "jest-puppeteer",
    "globalSetup": "jest-environment-puppeteer/setup",
    "globalTeardown": "jest-environment-puppeteer/teardown",
    "testEnvironment": "jest-environment-puppeteer",
    "globals": {
      "window": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged"
    }
  }
}
