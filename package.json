    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "jquery": "^3.2.1",
    "leaflet": "^1.2.0",
    "leaflet-ajax": "^2.1.0",
    "leaflet-dialog": "^1.0.3",
    "leaflet-draw": "^1.0.0",
    "leaflet-hash": "^0.2.1",
    "leaflet-modal": "^0.1.3",
