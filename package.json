{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "precommit": "lint-staged",
    "build":
      "webpack --mode production --config webpack.config.js --progress --profile --colors",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["prettier --write --insert-pragma", "git add"],
    "*.json": ["prettier --parser json --write", "git add"],
    "src/**.json": ["prettier --parser json --write", "git add"],
    "src/**/*.js": ["prettier --parser flow --write", "git add"],
    "src/**/*.flow": ["prettier --parser flow --write", "git add"],
    "src/*.js": ["prettier --parser flow --write", "git add"],
    "src/scss/*.scss": ["prettier --parser flow --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": ["osm"],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-flow": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/runtime": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.9",
    "flow-bin": "^0.70.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^2.1.1",
    "jest": "^22.0.0",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.6.1",
    "prettier": "^1.8.2",
    "prettier-eslint": "^8.2.2",
