{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --https --mode development",
    "precommit": "flow && lint-staged",
    "build":
      "webpack --mode production --config webpack.config.js --progress --profile --colors",
    "test": "flow && jest --runInBand"
  },
  "lint-staged": {
    "*.js": ["prettier --write --insert-pragma", "git add"],
    "*.json": ["prettier --parser json --write", "git add"],
    "src/**.json": ["prettier --parser json --write", "git add"],
    "src/**/*.js": ["prettier --parser flow --write", "git add"],
    "src/**/*.flow": ["prettier --parser flow --write", "git add"],
    "src/*.js": ["prettier --parser flow --write", "git add"],
    "src/scss/*.scss": ["prettier --parser flow --write", "git add"]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": ["osm"],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "@babel/runtime": "^7.0.0-beta.49",
    "babel-7-jest": "^21.3.3",
    "babel-loader": "^8.0.0-beta.0",
    "commitizen": "^2.10.1",
    "css-loader": "^1.0.0",
    "css-module-flow": "^1.0.0",
    "cssnano": "^4.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.9",
    "flow-bin": "^0.77.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^2.2.0",
    "jest": "^23.4.1",
    "jest-image-snapshot": "^2.4.2",
    "jest-puppeteer": "^3.0.1",
    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.4",
    "prettier-eslint": "^8.2.2",
    "puppeteer": "^1.4.0",
    "regenerator-runtime": "^0.12.0",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "sugarss": "^1.0.1",
    "tapable": "^1.0.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "jquery": "^3.2.1",
    "leaflet": "^1.2.0",
    "leaflet-ajax": "^2.1.0",
    "leaflet-dialog": "^1.0.3",
    "leaflet-draw": "^1.0.0",
    "leaflet-hash": "^0.2.1",
    "leaflet-modal": "^0.1.3",
    "leaflet-plugins": "^3.0.1",
    "leaflet.browser.print": "^0.6.7",
    "leaflet.photon": "^0.7.3",
    "material-design-iconic-font": "^2.2.0",
    "opensans-npm-webfont": "^1.0.0",
    "pouchdb": "^7.0.0",
    "ua-parser-js": "^0.7.18"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "modulePathIgnorePatterns": ["sandbox", "node_modules"],
    "transform": {
      "^.+\\.js?$": "babel-7-jest"
    },
    "preset": "jest-puppeteer",
    "globalSetup": "jest-environment-puppeteer/setup",
    "globalTeardown": "jest-environment-puppeteer/teardown",
    "testEnvironment": "jest-environment-puppeteer",
    "globals": {
      "window": true
    }
  }
}
