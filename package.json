{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "watch": "webpack --watch",
    "start": "webpack serve --open",
    "build": "npx webpack --mode production --config webpack.config.js --progress --color",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --maxConcurrency=1",
    "update-screenshots": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --maxConcurrency=1 -u"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --insert-pragma",
      "git add"
    ],
    "*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "src/*.js": [
      "prettier --write",
      "git add"
    ],
    "src/scss/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": [
    "osm"
  ],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "babel-loader": "9.2.1",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jest-puppeteer": "10.1.4",
    "lint-staged": "15.2.10",
    "mini-css-extract-plugin": "2.9.2",
    "postcss": "8.4.49",
    "postcss-loader": "8.1.1",
    "prettier": "3.3.3",
    "puppeteer": "23.8.0",
    "sass": "^1.71.1",
    "sass-loader": "16.0.3"
  },
  "dependencies": {
    "es6-promise": "4.2.8",
    "leaflet": "1.9.4",
    "leaflet-ajax": "2.1.0",
    "leaflet-dialog": "1.0.5",
    "leaflet-draw": "1.0.4",
    "leaflet-hash": "0.2.1",
    "leaflet-modal": "0.2.0",
    "leaflet.browser.print": "2.0.2",
    "leaflet.photon": "0.9.1",
    "material-design-iconic-font": "2.2.0",
    "opensans-npm-webfont": "1.0.0",
    "webpack": "^5.68.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "sandbox",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "preset": "jest-puppeteer",
    "globalSetup": "./setup.js",
    "globalTeardown": "./teardown.js",
    "testEnvironment": "./puppeteer_environment.js",
    "globals": {
      "window": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
