{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --https --mode development",
    "build": "webpack --mode production --config webpack.config.js --progress --profile --colors",
    "test": "flow && jest --runInBand"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --insert-pragma",
      "git add"
    ],
    "*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "src/**/*.flow": [
      "prettier --parser flow --write",
      "git add"
    ],
    "src/*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "src/scss/*.scss": [
      "prettier --parser flow --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": [
    "osm"
  ],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-transform-flow-strip-types": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/runtime": "7.3.4",
    "@babel/runtime-corejs2": "7.3.4",
    "babel-7-jest": "21.3.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "commitizen": "3.0.7",
    "css-loader": "2.1.0",
    "css-module-flow": "1.0.0",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.14.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "flow-bin": "0.94.0",
    "handlebars": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "imagemin-webpack-plugin": "2.4.2",
    "jest": "24.1.0",
    "jest-image-snapshot": "2.8.1",
    "jest-puppeteer": "4.0.0",
    "lint-staged": "8.1.4",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "prettier": "1.16.4",
    "prettier-eslint": "8.8.2",
    "puppeteer": "1.12.2",
    "regenerator-runtime": "0.13.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1"
  },
  "dependencies": {
    "es6-promise": "4.2.6",
    "jquery": "3.3.1",
    "leaflet": "1.4.0",
    "leaflet-ajax": "2.1.0",
    "leaflet-dialog": "1.0.5",
    "leaflet-draw": "1.0.4",
    "leaflet-hash": "0.2.1",
    "leaflet-modal": "0.1.5",
    "leaflet-plugins": "3.0.3",
    "leaflet.browser.print": "0.8.1",
    "leaflet.photon": "0.8.0",
    "material-design-iconic-font": "2.2.0",
    "opensans-npm-webfont": "1.0.0",
    "pouchdb": "7.0.0",
    "ua-parser-js": "0.7.19"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "sandbox",
      "node_modules"
    ],
    "transform": {
      "^.+\\.js?$": "babel-7-jest"
    },
    "preset": "jest-puppeteer",
    "globalSetup": "jest-environment-puppeteer/setup",
    "globalTeardown": "jest-environment-puppeteer/teardown",
    "testEnvironment": "jest-environment-puppeteer",
    "globals": {
      "window": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged"
    }
  }
}
