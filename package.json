{
  "name": "cad-killer",
  "version": "1.0.1",
  "description": "Visionneur de carte pour logisticiens press√©s",
  "main": "index.html",
  "scripts": {
    "watch": "webpack --watch",
    "start": "webpack serve --open",
    "build": "npx webpack --mode production --config webpack.config.js --progress --color",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --maxConcurrency=1",
    "update-screenshots": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --maxConcurrency=1 -u",
    "codeql:analyze": "codeql database analyze --format=sarif --output=codeql-results.sarif ./.codeql-db codeql-repo/javascript-queries"
  },
  "lint-staged": {
    "*.js": [
      "npx prettier --write --insert-pragma",
      "git add"
    ],
    "*.ts": [
      "npx prettier --write --insert-pragma",
      "git add"
    ],
    "*.tsx": [
      "npx prettier --write --insert-pragma",
      "git add"
    ],
    "*.json": [
      "npx prettier --parser json --write",
      "git add"
    ],
    "src/**.json": [
      "npx prettier --parser json --write",
      "git add"
    ],
    "src/**/*.js": [
      "npx prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "npx prettier --write",
      "git add"
    ],
    "src/**/*.tsx": [
      "npx prettier --write",
      "git add"
    ],
    "src/*.js": [
      "npx prettier --write",
      "git add"
    ],
    "src/scss/*.scss": [
      "npx prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-noblet/cad-killer.git"
  },
  "keywords": [
    "osm"
  ],
  "author": "Julien Noblet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julien-noblet/cad-killer/issues"
  },
  "homepage": "https://github.com/julien-noblet/cad-killer",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.27.6",
    "babel-loader": "^10.0.0",
    "codeql": "^1.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.5.1",
    "jest-puppeteer": "^11.0.0",
    "material-design-iconic-font": "^2.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.2.4",
    "puppeteer": "^24.11.2",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "leaflet": "1.9.4",
    "leaflet-ajax": "2.1.0",
    "leaflet-dialog": "1.0.5",
    "leaflet-draw": "1.0.4",
    "leaflet-hash": "0.2.1",
    "leaflet-modal": "0.2.0",
    "leaflet-plugins": "3.4.0",
    "leaflet.browser.print": "2.0.2",
    "leaflet.photon": "0.9.2",
    "opensans-npm-webfont": "1.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "sandbox",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "preset": "jest-puppeteer",
    "globalSetup": "./setup.js",
    "globalTeardown": "./teardown.js",
    "testEnvironment": "./puppeteer_environment.js",
    "globals": {
      "window": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
